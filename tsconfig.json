{
  "compilerOptions": {
      "target": "es2016",
      "outDir": "./build",
      "module": "commonjs",
      "types": ["node"],
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "strictNullChecks": true,


    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"]
    }

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
}
